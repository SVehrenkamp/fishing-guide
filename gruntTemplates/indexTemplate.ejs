<!DOCTYPE html>
<html>
<head>

    <title>Target Racing</title>

    <link href="<%-cssPath%>" media='all' rel='stylesheet' type='text/css' />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="<%-jsPath%>"></script>

    <script src="<%-basePath%>js/vendor/picturefill.js"></script>

    <script>

        // Picture element HTML5 shiv
        document.createElement( "picture" );

        // pass in any configuration options to the application
        var devOptions = {};
        devOptions.whatEver = "pass static values to the app on startup";
        devOptions.basePath = "<%-basePath%>";
        devOptions.imagePath = "<%-basePath%>";
        devOptions.entryId = "test";


        function load() {

            //Load common code that includes config, then load the app
            //logic for this page. Do the require calls here instead of
            //a separate file so after a build there are only 2 HTTP
            //requests instead of three.
            require(["<%-requirePath%>"], function(common) {
                //js/common sets the baseUrl to be js/ so
                //can just ask for 'app/main1' here instead
                //of 'js/app/main1'
                //require(['packages/package1']);

                require(['app']);

            });

        }
    </script>

</head>
<body onload="load()">

    <div id="dev">
        <div class="preloader">
            <div class="center">
                <img src="http://tgtfiles.target.com/race/dev-images/spinner.gif">
            </div>
        </div>
    </div>


    

</body>
</html>